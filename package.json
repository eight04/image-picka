{
  "title": "Image Picka",
  "name": "image-picka",
  "version": "0.11.2",
  "description": "An image picker written in webextensions",
  "author": "eight04 <eight04@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "lint": "eslint \"**/*.js\" --cache",
    "test": "npm run lint && npm run build && web-ext lint",
    "start": "web-ext run",
    "preversion": "npm test",
    "build": "sync-version src/static/manifest.json && shx rm -rf build/* && rollup -c",
    "build-locales": "tx pull -a --mode=developer && webext-tx-fix -s extension",
    "postversion": "web-ext build && git push --follow-tags",
    "server": "node test/server.js",
    "travis": "npm test && npm run build && npm run test-webext"
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@eight04/idb-storage": "^0.4.2",
    "content-disposition": "^0.5.3",
    "eslint": "^6.8.0",
    "expression-eval": "^2.0.0",
    "fast-glob": "^3.1.1",
    "mime": "^2.4.4",
    "node-sigint": "0.0.1",
    "rollup": "^1.27.14",
    "rollup-plugin-cjs-es": "^0.9.0",
    "rollup-plugin-copy-glob": "^0.3.1",
    "rollup-plugin-iife": "^0.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-shim": "^1.0.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.1.3",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "stream-throttle": "^0.1.3",
    "sync-version": "^1.0.1",
    "web-ext": "^4.0.0",
    "webext-menus": "^0.2.0",
    "webext-pref": "^0.6.0",
    "webext-pref-ui": "^0.1.1",
    "webext-tx-fix": "^0.3.2",
    "webextension-polyfill": "^0.6.0"
  },
  "repository": "eight04/image-picka"
}
