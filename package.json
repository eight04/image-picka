{
  "title": "Image Picka",
  "name": "image-picka",
  "version": "0.9.4",
  "description": "An image picker written in webextensions",
  "author": "eight04 <eight04@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "webext-menus": "^0.1.3",
    "webextension-polyfill": "^0.3.1"
  },
  "scripts": {
    "lint": "eslint \"**/*.js\" --cache --ignore-pattern extension/vendor",
    "test": "npm run lint && web-ext lint",
    "start": "web-ext run",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "build": "sync-version extension/manifest.json",
    "build-locales": "tx pull -a --mode=developer && webext-tx-fix -s extension",
    "build-vendor": "shx cp node_modules/webext-menus/dist/webext-menus.min.js extension/vendor/webext-menus && shx cp node_modules/webextension-polyfill/dist/browser-polyfill.min.js extension/vendor/webextension-polyfill",
    "postversion": "web-ext build && git push --follow-tags",
    "server": "node test/server.js"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "mime": "^2.3.1",
    "node-sigint": "0.0.1",
    "shx": "^0.3.2",
    "stream-throttle": "^0.1.3",
    "sync-version": "^1.0.1",
    "web-ext": "^2.9.1",
    "webext-tx-fix": "^0.3.1"
  }
}
